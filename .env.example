# Homelab Configuration
# Copy this file to .env and fill in your values
# cp .env.example .env

# === NETWORK ===
GATEWAY_IP=192.168.x.1
CONTROL_PLANE_IP=192.168.x.201
WORKER_01_IP=192.168.x.202
WORKER_02_IP=192.168.x.203

# Subnet (e.g., 192.168.178.0/24)
NETWORK_SUBNET=192.168.x.0/24

# === KUBERNETES ===
K8S_API_SERVER=${CONTROL_PLANE_IP}
K8S_API_PORT=6443
K8S_VERSION=1.29.2

# Pod and Service CIDRs
POD_CIDR=10.200.0.0/16
SERVICE_CIDR=10.32.0.0/24
CLUSTER_DNS=10.32.0.10

# === CILIUM ===
CILIUM_VERSION=1.17.0

# === SSH ===
SSH_USER=admin
SSH_KEY_PATH=~/.ssh/id_ed25519

# === GIT ===
GIT_REPO_URL=https://github.com/YOUR-USERNAME/homelab.git
